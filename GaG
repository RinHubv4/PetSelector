-- Rin Trade System V3.5 â€” Centered Cyberpunk Mobile Edition
-- Place as a LocalScript in StarterPlayerScripts or StarterGui
-- Visual-only: centered layout, Players tab (avatar + selectable glow), Controls tab (sliding toggles), minimize, mobile scaling

-- Services
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local localPlayer = Players.LocalPlayer
local playerGui = localPlayer:WaitForChild("PlayerGui")

-- Helper to create instances with properties
local function New(class, props)
	local obj = Instance.new(class)
	if props then
		for k, v in pairs(props) do
			if k == "Parent" then
				obj.Parent = v
			else
				obj[k] = v
			end
		end
	end
	return obj
end

-- Theme colors (Black & White edition)
local BG = Color3.fromRGB(10, 10, 10)          -- background (deep black)
local PANEL = Color3.fromRGB(10, 10, 10)       -- panel
local PURPLE = Color3.fromRGB(255, 255, 255)   -- replaced with pure white accent
local CYAN = Color3.fromRGB(200, 200, 200)     -- soft gray highlight
local TEXT = Color3.fromRGB(240, 240, 240)     -- main text (white)
local SUB = Color3.fromRGB(160, 160, 160)      -- secondary text (gray)


-- Root GUI
local screenGui = New("ScreenGui", {
	Name = "Rin Scripts",
	Parent = playerGui,
	ResetOnSpawn = false,
	IgnoreGuiInset = true,
	DisplayOrder = 9999,
	ZIndexBehavior = Enum.ZIndexBehavior.Sibling
})

-- Root frame (centered)
local root = New("Frame", {
	Parent = screenGui,
	Name = "Root",
	AnchorPoint = Vector2.new(0.5, 0.5),
	Position = UDim2.new(0.5, 0, 0.5, 0),
	Size = UDim2.new(0, 680, 0, 380),
	BackgroundColor3 = BG,
	BorderSizePixel = 0,
	Active = true,
	Draggable = false,
})
New("UICorner", {Parent = root, CornerRadius = UDim.new(0, 16)})
New("UIStroke", {Parent = root, Thickness = 1.4, Color = Color3.fromRGB(60, 36, 90)})

-- Responsive UIScale for mobile
local uiScale = New("UIScale", {Parent = root})
local function updateScale()
	local sw = workspace.CurrentCamera and workspace.CurrentCamera.ViewportSize.X or 1280
	if sw < 420 then
		uiScale.Scale = 0.72
	elseif sw < 720 then
		uiScale.Scale = 0.86
	else
		uiScale.Scale = 1
	end
end
updateScale()
if workspace.CurrentCamera then
	workspace.CurrentCamera:GetPropertyChangedSignal("ViewportSize"):Connect(updateScale)
end

